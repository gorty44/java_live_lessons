package oop;

import java.util.HashMap;

public class Test {
	private static HashMap<SomeKey, String> map = new HashMap<>();

	private class SomeKey {
		private int parameter;
		
		public SomeKey(int param) {
			parameter = param;
		}
		
		@Override
		public int hashCode() {
			return -1;
		}

		@Override
		public boolean equals(Object o) {
			if (this == o)
				return true;
			if (o == null)
				return false;

			SomeKey sk = (SomeKey) o;
			if (this.parameter == sk.parameter)
				return true;
			return false;
		}
	}
	
	public static void main(String[] args) {
		SomeKey sk1 = new Test().new SomeKey(1);
		SomeKey sk2 = new Test().new SomeKey(2);
		SomeKey sk3 = new Test().new SomeKey(3);
		SomeKey sk4 = new Test().new SomeKey(4);
		SomeKey sk5 = new Test().new SomeKey(5);
		SomeKey sk6 = new Test().new SomeKey(6);
		
		map.put(sk1, "1");
		map.put(sk2, "2");
		map.put(sk3, "3");
		map.put(sk4, "4");
		map.put(sk5, "5");
		map.put(sk6, "6");
		
		//System.out.println(map.size());
		System.out.println(map.get(sk6));
	}
}
